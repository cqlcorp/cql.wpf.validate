<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:sys="clr-namespace:System;assembly=mscorlib">

	<sys:String x:Key="Validation:AlphaNumeric">{0} consist of letters and numbers!</sys:String>
	<sys:String x:Key="Validation:Length:Min">{0} must be at least {1} characters in length!</sys:String>
	<sys:String x:Key="Validation:Length:Max">{0} must be no more than {1} characters in length!</sys:String>
	<sys:String x:Key="Validation:Regex">{0} does not appear to be a valid value!</sys:String>
	<sys:String x:Key="Validation:Range:Between">{0} must be between {1} and {2}!</sys:String>
	<sys:String x:Key="Validation:Range:GreaterThan">{0} must be greater than {1}!</sys:String>
	<sys:String x:Key="Validation:Range:LessThan">{0} must be less than {1}!</sys:String>
	<sys:String x:Key="Validation:Required">{0} is required!</sys:String>
	
</ResourceDictionary>